# Server
PORT=3000
NODE_ENV=development

# CORS
ALLOWED_ORIGINS=http://localhost:19006,http://localhost:19000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=me_mantra_db
DB_USER=postgres
DB_PASSWORD=

# Postgres container init (used by docker compose db service)
# These are read by the postgres image on first init ONLY.
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=me_mantra_db
POSTGRES_DB=me_mantra_db
PGADMIN_CONFIG_SERVER_MODE='true'

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=
JWT_EXPIRES_IN=7d

# MinIO
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin

# Firebase (FCM)
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
FIREBASE_PRIVATE_KEY=

# OAuth
GOOGLE_CLIENT_ID=
