{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", "ios/build/**", "android/app/build/**"]
    },
    "dev": { "cache": false, "persistent": true },
    "lint": {},
    "typecheck": {},
    "test": {
      "cache": false,
      "outputs": ["coverage/**", "test-results/**"]
    }
  }
}
